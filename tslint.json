{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-shadowed-variable": false,
        "no-var-requires": false,
        "no-console": [false],
        "object-literal-shorthand": true,
        "variable-name": [false],
        "member-access":false,
        "no-bitwise":false,
        "object-literal-sort-keys": false,
        "ordered-imports": [
            false
        ],
        "max-classes-per-file": [
            false
        ]
    },
    "rulesDirectory": []
}